/* pages/integralDetail/detail.wxss */
.integral-page{
  background: #f5f5f5;
  height: 100vh;
  font-size: 28rpx;
  overflow-y: auto;
}
.integral-page .integral-list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #fff;
  padding: 5px 10px;
  border-bottom: 1px solid #f8f8f9;
  min-height: 40px;
}

.integral-page .integral-list>view:nth-child(1) .date{
  font-size: 22rpx;
}
.integral-page .integral-list>view:nth-child(2){
  color: #2d8cf0;
}