.Auth-header {
    margin:0 30rpx 30rpx 30rpx;
    border-bottom: 1px solid #ccc;
    text-align: center;
    height: 300rpx;
    line-height: 450rpx;
}
 
.Auth-header image {
    width: 150rpx;
    height: 40rpx;
}
 
.Auth-content {
    margin-left: 30rpx;
    margin-bottom: 60rpx;
    font-size: 14px;
}
 
.Auth-content text {
    display: block;
    color: #9d9d9d;
    margin-top: 20rpx;
}
 
.Auth-bottom {
    border-radius: 80rpx;
    margin: 70rpx 50rpx;
    font-size: 30rpx;
}
